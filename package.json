    "bin/",
    "dist/",
    "mcp/",
    "utils/",
    "scripts/postinstall.js",
    "server.js",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "start": "node server.js",
    "up": "node bin/cipher.js up",
